{
    "name": "bve5-language-support",
    "displayName": "Bve5 Language Support",
    "description": "BveTrainsim5.7 language support extension for VSCode.",
    "version": "0.3.0",
    "publisher": "aoisupersix",
    "icon": "icons/extension-icon.png",
    "author": {
        "email": "aoisupersix@gmail.com",
        "name": "aoisupersix",
        "url": "http://aoisupersix.tokyo"
    },
    "license": "The MIT License(MIT)",
    "repository": {
        "type": "git",
        "url": "https://github.com/aoisupersix/vscode-bve5-language-support"
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Other"
    ],
    "galleryBanner": {
        "color": "#EFF1F3",
        "theme": "dark"
    },
    "activationEvents": [
        "onLanguage:bve-map-2.02"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [],
        "languages": [
            {
                "id": "bve-map-2.02",
                "aliases": [
                    "BveTs Map 2.02",
                    "bve5-map"
                ],
                "firstLine": "^BveTs\\s*Map\\s*2.02",
                "configuration": "./lang_config/bve5-map-configuration.json",
                "extensions": [
                    ".txt"
                ],
                "filenames": [
                    "MAP",
                    "Map",
                    "map"
                ]
            }
        ],
        "grammars": [
            {
                "language": "bve-map-2.02",
                "scopeName": "source.bve-map-2.02",
                "path": "./syntaxes/bve-map-2.02.json"
            }
        ],
        "snippets": [
            {
                "language": "bve-map-2.02",
                "path": "./snippets/bve-map-2.02.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "linqts": "^1.11.0"
    }
}
