{
  "scopeName": "source.bve-map-2.02",
  "name": "BveTs-Map-2.02",
  "patterns": [
    {
      "name": "header.bve-map-2.02",
      "match": "(?imx-imx)(?i:(BveTs\\s*Map\\s*2.02))",
      "captures": {
        "1": {
          "name": "keyword.other.bve-map-2.02"
        }
      }
    },
    {
      "name": "comment.bve-map-2.02",
      "match": "((//|#).*)",
      "captures": {
        "1": {
          "name": "comment.line.bve-map-2.02"
        }
      }
    },
    {
      "name": "string.bve-map-2.02",
      "match": "'(.*?)'",
      "captures": {
        "1": {
          "name": "string.quoted.single.bve-map-2.02"
        }
      }
    },   
    {
      "name": "mapelement.bve-map-2.02",
      "match": "(?imx-imx)(?i:(curve|gradient|track|structure|repeater|background|station|section|signal|beacon|speedlimit|pretrain|light|fog|drawdistance|drawdistance|cabilluminance|irregularity|adhesion|sound|sound3d|rollingnoise|flangenoise|jointnoise|train|x|y|cant|include))",
      "captures": {
        "1": {
          "name": "support.class.bve-map-2.02"
        }
      }
    },
    {
      "name": "function.bve-map-2.02",
      "match": "(?imx-imx)(?i:(setgauge|setcenter|setfunction|begintransition|begin0|end|interpolate|change|position|load|putbetween|put0|put|begin|pass|ambient|diffuse|direction|play|add|enable|stop))",
      "captures": {
        "1": {
          "name": "support.function.bve-map-2.02"
        }
      }
    },
    {
      "name": "numeric.bve-map-2.02",
      "match": "(\\d+(?i:\\.\\d+)?)",
      "captures": {
        "1": {
          "name": "constant.numeric.bve-map-2.02"
        }
      }
    },
    {
      "name": "operator.bve-map-2.02",
      "match": "([+\\-*/%])",
      "captures": {
        "1": {
          "name": "keyword.operator.bve-map-2.02"
        }
      }
    },
    {
      "name": "math-function.bve-map-2.02",
      "match": "(?imx-imx)(?i:(abs|atan2|ceil|cos|exp|floor|log|pow|land|sin|sqrt))",
      "captures": {
        "1": {
          "name": "support.function.bve-map-2.02"
        }
      }
    },
    {
      "name": "variable.bve-map-2.02",
      "match": "(\\$)(\\w+)",
      "captures": {
        "1": {
          "name": "storage.type.bve-map-2.02"
        },
        "2": {
          "name": "variable.parameter.bve-map-2.02"
        }
      }
    },
    {
      "name": "distance.bve-map-2.02",
      "match": "(?imx-imx)(?i:(distance))",
      "captures": {
        "1": {
          "name": "support.constant.bve-map-2.02"
        }
      }
    }
  ]
}